{
  "window.title": "${workspaceFolder}",
  "prettier.enable": false,
  "editor.formatOnSave": false,
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": "always"
  },
  "cSpell.words": [
    "lcov",
    "nonblock",
    "tsup"
  ],
  "nodejs-testing.verbose": true,
  "nodejs-testing.concurrency": 5,
  "nodejs-testing.extensions": [
    {
      "extensions": [
        "mjs",
        "cjs",
        "js"
      ],
      "parameters": []
    },
    {
      "extensions": [
        "ts"
      ],
      "filePatterns": [
        "**/*.test.ts"
      ],
      "parameters": [
        "--import",
        "tsx"
      ]
    }
  ],
}